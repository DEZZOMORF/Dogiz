<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="CardViewPager">
        <attr name="pagerName" format="string"/>
        <attr name="pagerButtonVisibility" format="boolean"/>
        <attr name="title" format="string"/>
        <attr name="text" format="string"/>
        <attr name="cardButtonVisibility" format="boolean"/>
        <attr name="style">
            <enum name="simple" value="0" />
            <enum name="notification" value="1" />
            <enum name="bigImage" value="2" />
            <enum name="bigButton" value="3" />
        </attr>
    </declare-styleable>
</resources>